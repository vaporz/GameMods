                [
                    {
                        "index": 0,
                        "position": { "x": 0.25, "y": 0.5, "z": 0 },
                        "thrustDirection": { "x": 0, "y": -1, "z": 0 },
                        "maxThrust": 150,
                        "gimbalAngle": 5,
                        "propellants": [
                            { "name":"fuel", "flow": 0.2 },
                            { "name":"oxidizer", "flow": 0.6 }
                        ],
                        "audio": {
                            "events": {
                                "activated": "engine_activate.mp3",
                                "running": "engine_working.mp3",
                                "flameout": "flameout.mp3"
                            }
                        }
                    }
                ]
            ]
        }
    },
}