{
    "partName": "solid",
    "prefabName": "Assets/Prefabs/Parts/SolidBooster.prefab",
    "type": "engine",
    "dryMass": 1,
    "plugins": {
        "container": {
            "capacity": 5
        },
        "engine": {
            "allowShutdown": false,
            "throttlable": false,
            "selfFeeding": true,
            "nozzleModes":
